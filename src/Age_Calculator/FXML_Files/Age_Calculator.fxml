<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="main" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Age_Calculator.Java_Cts.Age_Calculator_Ct">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="400.0" stylesheets="@../Design/Design.css">
         <children>
            <Label fx:id="description" layoutX="135.0" layoutY="14.0" text="Age Calculator" underline="true">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <JFXButton fx:id="calculate" layoutX="256.0" layoutY="320.0" onAction="#Calculate" prefHeight="66.0" prefWidth="130.0" text="Calculate">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="exit" layoutX="14.0" layoutY="320.0" onAction="#Exit" prefHeight="66.0" prefWidth="130.0" text="Exit">
               <font>
                  <Font size="16.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="reset" layoutX="14.0" layoutY="14.0" onAction="#Reset" prefHeight="30.0" prefWidth="36.0" text="R" />
            <JFXButton fx:id="help" layoutX="350.0" layoutY="13.5" onAction="#Help" prefHeight="30.0" prefWidth="36.0" text="?" />
            <Label fx:id="answer" alignment="CENTER" layoutX="42.0" layoutY="228.0" prefHeight="41.0" prefWidth="317.0">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <JFXTextField fx:id="from" alignment="CENTER" focusColor="LIME" layoutX="56.0" layoutY="88.0" prefHeight="28.0" prefWidth="295.0" promptText="Enter the year in which you born" unFocusColor="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </JFXTextField>
            <JFXTextField fx:id="current" alignment="CENTER" focusColor="LIME" layoutX="56.0" layoutY="155.0" prefHeight="28.0" prefWidth="295.0" promptText="Enter the year from which you want to count" unFocusColor="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
            </JFXTextField>
         </children>
      </AnchorPane>
   </children>
</StackPane>
